{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.0",
  "default_locale": "en",
  "permissions": [
    "proxy",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_defaultTitle__"
  }
}